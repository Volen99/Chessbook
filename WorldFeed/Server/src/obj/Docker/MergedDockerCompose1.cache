services:
  worldfeed.history.bc.science:
    build:
      context: C:\Users\volen\OneDrive\Desktop\WorldFeed\WorldFeed\Server
      dockerfile: History/BC/Science/WorldFeed.History.BC.Science/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WorldFeed.History.BC.Science
      target: base
    container_name: WorldFeed.History.BC.Science
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: worldfeedhistorybcscience:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/WorldFeed.History.BC.Science.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\volen\OneDrive\Desktop\WorldFeed\WorldFeed\Server\History\BC\Science\WorldFeed.History.BC.Science:/app:rw
    - C:\Users\volen\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\volen\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\volen\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\volen\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\volen\OneDrive\Desktop\WorldFeed\WorldFeed\Server:/src:rw
